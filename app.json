{
  "expo": {
    "name": "DevSixTech",
    "slug": "DevSixTech",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/devsix.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

   "splash": {
      "image": "./assets/images/devsix.jpg",  
      "backgroundColor": "#084c3c",      
      "resizeMode": "contain",
      "imageWidth": 200
    },

    "ios": {
      "supportsTablet": true,
      "image": "./assets/images/devsix.jpg",  
      "backgroundColor": "#084c3c",      
      "resizeMode": "contain",
      "imageWidth": 200
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/devsix2.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/devsix2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/devsix.jpg",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
